{
  "parserOptions": {
    "ecmaVersion": 2021,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "eslint-config-prettier"],
  "rules": {
    "react/prop-types": "warn",
    "no-unused-vars": "warn"
  },
  "env": {
    "es6": true,
    "browser": true,
    "jest": true,
    "node": true
  }
}